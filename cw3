def solution(inputString):
    # Solution 1:
    a = 0
    b = len(inputString)-1
    while a <= b:
        if inputString[a] != inputString[b]:
            return False
        a += 1
        b -= 1
    return True

    # Solution 2:
    # revString = inputString[::-1]
    # if inputString == revString:
    #     return True
    # return False


print(solution("ahha"))
print(solution("baaa"))
